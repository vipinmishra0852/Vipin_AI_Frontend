{
  "name": "codex-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run build:css --watch\" \"vite\"",
    "build": "vite build",
    "preview": "vite preview",
    "build:css": "tailwindcss build style.css -o dist/styles.css",
    "build:css:watch": "tailwindcss -i style.css -o dist/styles.css --watch"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.0.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "vite": "^4.0.0"
  }
}
